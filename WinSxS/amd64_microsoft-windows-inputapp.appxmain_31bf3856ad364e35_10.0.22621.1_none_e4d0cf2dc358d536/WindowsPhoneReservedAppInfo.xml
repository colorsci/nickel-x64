<?xml version="1.0" encoding="utf-8"?>
<WindowsPhoneReservedAppInfo xmlns="http://schemas.microsoft.com/phone/2013/windowsphonereservedappinfo">
    <SoftwareCapabilities>
        <SoftwareCapability Id="ID_CAP_CHAMBER_PROFILE_CODE_R" />
        <SoftwareCapability Id="ID_CAP_CHAMBER_PROFILE_DATA_R" />
        <SoftwareCapability Id="ID_CAP_SYSTEM_REGISTRAR" />

        <!-- Needed to listen to changes in the state of the location WNF event WNF_LFS_STATE -->
        <SoftwareCapability Id="ID_CAP_LOCATION" />
        <!-- ShellEvents required Cell Core Capabilities, to listen to various cell core WNF states -->
        <SoftwareCapability Id="ID_CAP_CELL_WNF" />
        <!-- Required to listen to VPN WNF -->
        <SoftwareCapability Id="ID_CAP_NETWORKING_VPN_ADMIN" />
        <!-- For accessing Do Not Disturb state -->
        <SoftwareCapability Id="ID_CAP_DO_NOT_DISTURB" />
        <!-- For accessing IME state -->
        <SoftwareCapability Id="ID_CAP_INPUT_CORE" />
        <!-- Phone API and shared registry keys -->
        <SoftwareCapability Id="ID_CAP_PHONE_INTERNAL" />
        <!-- Legacy Phone shutdown capability -->
        <SoftwareCapability Id="ID_CAP_BUILTIN_SHUTDOWN" />
        <!-- Access to Dua Api and DU Core registries -->
        <SoftwareCapability Id="ID_CAP_DU_CORE_API" />
        <!-- Additional capabilities needed for calling components -->
        <SoftwareCapability Id="ID_CAP_COMMS_APPLICATIONS" />
        <SoftwareCapability Id="ID_CAP_COMMS_COMMON" />
        <SoftwareCapability Id="ID_CAP_COMMS_SETTINGS" />
        <!-- For volume and sound settings WNF and registry keys-->
        <SoftwareCapability Id="ID_CAP_AUDIO_INTERNAL" />
        <SoftwareCapability Id="ID_CAP_SOUND_CONTROL" />
    </SoftwareCapabilities>
</WindowsPhoneReservedAppInfo>
