{
  "PredefinedRegex": [
    {
      "Name": "MatchAnyFilesRecursive",
      "Regex": "(?:(?!\\\\)(?![\\s\\S]*((\\.\\.|\\s(\\\\|$))|[>\\/:?<|\"]|\\\\\\\\)))"
    },
    {
      "Name": "MatchAnyExtension",
      "Regex": "[^\\\\>\\/:?<|\"]+\\.{1}[^\\\\>\\/:?<|\".]+$"
    }
  ],
  "Deny": [
    {
      "Name": "UserDocumentExtension",
      "Regex": "^.*\\.(?:doc|docx|dot|wbk|docm|dotx|dotm|docb|xls|xlt|xlt|xlm|xlsx|xlsm|xltx|xltm|xlsb|xla|xlam|xll|xlw|ppt|pot|pps|pptx|pptm|potx|potm|ppam|ppam|ppsx|ppsm|sldx|sldm|pdf|accdb|rar)$"
    },
    {
      "Name": "ImageExtension",
      "Regex": "^.*\\.(?:jpeg|png|jpg|gif|bmp|tif|tiff|svg|ico|jfif|exif|ppm|pgm|pbm|pnm)$"
    },
    {
      "Name": "MediaExtension",
      "Regex": "^.*\\.(?:mp3|mp4|wav|raw|wma|m4a|mov|avi|wmv)$"
    }
  ],
  "Allow": [
    {
      "Regex": "%ProgramFiles%\\\\$(MatchAnyFilesRecursive)"
    },
    {
      "Regex": "%ProgramFiles(x86)%\\\\$(MatchAnyFilesRecursive)"
    },
    {
      "Regex": "%windir%\\\\$(MatchAnyFilesRecursive)"
    },
    {
      "Regex": "%programdata%\\\\Microsoft\\\\Windows\\\\AppRepository\\\\$(MatchAnyFilesRecursive)"
    },
    {
      "Regex": "[^.\\\\>\\/:?<|\"]+:\\\\WindowsApps\\\\$(MatchAnyFilesRecursive)"
    },
    {
      "Regex": "[^.\\\\>\\/:?<|\"]+:\\\\DeliveryOptimization\\\\$(MatchAnyFilesRecursive)"
    },
    {
      "Regex": "[^.\\\\>\\/:?<|\"]+:\\\\WUDownloadCache\\\\$(MatchAnyFilesRecursive)"
    },
    {
      "Regex": "%localappdata%\\\\Packages\\\\$(MatchAnyFilesRecursive)"
    },
    {
      "Regex": "%localappdata%\\\\Google\\\\Chrome\\\\User Data\\\\Default\\\\Extensions\\\\gghenmaodhhklgdblbcbebeekbkcgfgp\\\\$(MatchAnyExtension)"
    },
    {
      "Regex": "%localappdata%\\\\Google\\\\Chrome\\\\User Data\\\\Default\\\\Extensions\\\\lccllplkfcdlafcmifofcpgbldnadkha\\\\$(MatchAnyExtension)"
    },
    {
      "Regex": "%localappdata%\\\\Google\\\\Chrome\\\\User Data\\\\Default\\\\Extensions\\\\mfjnknhkkiafjajicegabkbimfhplplj\\\\$(MatchAnyExtension)"
    },
    {
      "Regex": "%systemdrive%\\\\Data\\\\Programs\\\\WindowsApps\\\\$(MatchAnyFilesRecursive)"
    },
    {
      "Regex": "%systemdrive%\\\\Data\\\\Windows\\\\rescache\\\\$(MatchAnyFilesRecursive)"
    },
    {
      "Regex": "%localappdata%\\\\PackagesStaging\\\\$(MatchAnyFilesRecursive)"
    },
    {
      "Regex": "[A-Za-z]{1}:\\\\WpSystem\\\\S-[0-9\\-]+\\\\$(MatchAnyFilesRecursive)"
    },
    {
      "Regex": "%userprofile%\\\\AppData\\\\Local\\\\Apps\\\\Remote Desktop\\\\$(MatchAnyFilesRecursive)"
    },
    {
      "Regex": "%userprofile%\\\\AppData\\\\Local\\\\Programs\\\\Remote Desktop\\\\$(MatchAnyFilesRecursive)"
    },
    {
      "Regex": "%localappdata%\\\\Publishers\\\\$(MatchAnyFilesRecursive)"
    },
    {
      "Regex": "[^.\\\\>\\/:?<|\"]+:\\\\Program Files\\\\ModifiableWindowsApps\\\\$(MatchAnyFilesRecursive)"
    },
    {
      "Regex": "%localappdata%\\\\Microsoft\\\\Windows\\\\Clipboard\\\\$(MatchAnyFilesRecursive)"
    }
  ]
}