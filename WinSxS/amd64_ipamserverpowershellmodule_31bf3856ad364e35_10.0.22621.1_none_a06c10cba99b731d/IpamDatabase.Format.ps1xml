        <TableRowEntries>
          <TableRowEntry>
            <TableColumnItems>
              <TableColumnItem>
                <PropertyName>DatabaseType</PropertyName>
              </TableColumnItem>
              <TableColumnItem>
                <PropertyName>WidSchemaPath</PropertyName>
              </TableColumnItem>
              <TableColumnItem>
                <PropertyName>DatabaseServer</PropertyName>
              </TableColumnItem>
              <TableColumnItem>
                <PropertyName>DatabaseName</PropertyName>
              </TableColumnItem>
              <TableColumnItem>
                <PropertyName>DatabasePort</PropertyName>
              </TableColumnItem>
              <TableColumnItem>
