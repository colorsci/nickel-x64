<Configuration>
  <SelectionSets>
    <SelectionSet>
      <Name>IpamDatabase</Name>
      <Types>
        <TypeName>Microsoft.Management.Infrastructure.CimInstance#MSFT_IPAM_DBConfig</TypeName>
      </Types>
    </SelectionSet>
  </SelectionSets>

  <ViewDefinitions>
    <View>
      <Name>IpamDatabaseListView</Name>
      <ViewSelectedBy>
        <SelectionSetName>IpamDatabase</SelectionSetName>
      </ViewSelectedBy>
      <ListControl>
        <ListEntries>
          <ListEntry>
            <ListItems>
              <ListItem>
                <PropertyName>DatabaseType</PropertyName>
              </ListItem>
              <ListItem>
                <PropertyName>WidSchemaPath</PropertyName>
              </ListItem>
              <ListItem>
                <PropertyName>DatabaseServer</PropertyName>
              </ListItem>
              <ListItem>
                <PropertyName>DatabaseName</PropertyName>
              </ListItem>
              <ListItem>
                <PropertyName>DatabasePort</PropertyName>
              </ListItem>
              <ListItem>
                <PropertyName>DatabaseAuthType</PropertyName>
              </ListItem>
              <ListItem>
                <PropertyName>DatabaseUser</PropertyName>
              </ListItem>
            </ListItems>
          </ListEntry>
        </ListEntries>
      </ListControl>
    </View>

    <View>
      <Name>IpamDatabaseTable</Name>
