<Configuration>
  <SelectionSets>
    <SelectionSet>
      <Name>IpamBlock</Name>
      <Types>
        <TypeName>Microsoft.Management.Infrastructure.CimInstance#MSFT_IPAM_Block</TypeName>
      </Types>
    </SelectionSet>
  </SelectionSets>

  <ViewDefinitions>
    <View>
      <Name>IpamBlockListView</Name>
      <ViewSelectedBy>
        <SelectionSetName>IpamBlock</SelectionSetName>
      </ViewSelectedBy>
      <ListControl>
        <ListEntries>
          <ListEntry>
            <ListItems>
              <ListItem>
                <PropertyName>Utilization</PropertyName>
              </ListItem>
              <ListItem>
                <PropertyName>NetworkId</PropertyName>
              </ListItem>
              <ListItem>
                <PropertyName>StartIPAddress</PropertyName>
              </ListItem>
              <ListItem>
                <PropertyName>EndIPAddress</PropertyName>
              </ListItem>
              <ListItem>
                <PropertyName>Rir</PropertyName>
              </ListItem>
              <ListItem>
                <PropertyName>LastAssignedDate</PropertyName>
              </ListItem>
              <ListItem>
                <PropertyName>TotalAddresses</PropertyName>
              </ListItem>
              <ListItem>
                <PropertyName>AssignedAddresses</PropertyName>
              </ListItem>
              <ListItem>
                <PropertyName>UtilizedAddresses</PropertyName>
              </ListItem>
              <ListItem>
                <PropertyName>PercentageUtilized</PropertyName>
              </ListItem>
              <ListItem>
                <PropertyName>AddressCategory</PropertyName>
              </ListItem>
              <ListItem>
                <PropertyName>Owner</PropertyName>
              </ListItem>
              <ListItem>
                <PropertyName>Description</PropertyName>
              </ListItem>
              <ListItem>
                <PropertyName>RirReceivedDate</PropertyName>
              </ListItem>
              <ListItem>
                <PropertyName>PercentageAssigned</PropertyName>
              </ListItem>
            </ListItems>
          </ListEntry>
        </ListEntries>
      </ListControl>
    </View>

    <View>
      <Name>IpamBlockTable</Name>
      <ViewSelectedBy>
        <SelectionSetName>IpamBlock</SelectionSetName>
      </ViewSelectedBy>
      <TableControl>
        <TableHeaders>
          <TableColumnHeader>
            <label>NetworkId</label>
          </TableColumnHeader>
          <TableColumnHeader>
            <label>StartIPAddress</label>
          </TableColumnHeader>
          <TableColumnHeader>
            <label>EndIPAddress</label>
          </TableColumnHeader>
          <TableColumnHeader>
            <label>Rir</label>
          </TableColumnHeader>
        </TableHeaders>
        <TableRowEntries>
          <TableRowEntry>
            <TableColumnItems>
              <TableColumnItem>
                <PropertyName>NetworkId</PropertyName>
              </TableColumnItem>
              <TableColumnItem>
                <PropertyName>StartIPAddress</PropertyName>
              </TableColumnItem>
              <TableColumnItem>
                <PropertyName>EndIPAddress</PropertyName>
              </TableColumnItem>
              <TableColumnItem>
                <PropertyName>Rir</PropertyName>
              </TableColumnItem>
            </TableColumnItems>
          </TableRowEntry>
        </TableRowEntries>
      </TableControl>
    </View>
  </ViewDefinitions>
</Configuration>