<Configuration>
  <SelectionSets>
    <SelectionSet>
      <Name>IpamSubnet</Name>
      <Types>
        <TypeName>Microsoft.Management.Infrastructure.CimInstance#MSFT_IPAM_Subnet</TypeName>
      </Types>
    </SelectionSet>
    <SelectionSet>
      <Name>IpamFreeSubnet</Name>
      <Types>
        <TypeName>Microsoft.Management.Infrastructure.CimInstance#MSFT_IPAM_FreeSubnet</TypeName>
      </Types>
    </SelectionSet>
  </SelectionSets>

  <ViewDefinitions>
    <View>
      <Name>IpamSubnetListView</Name>
      <ViewSelectedBy>
        <SelectionSetName>IpamSubnet</SelectionSetName>
      </ViewSelectedBy>
      <ListControl>
        <ListEntries>
          <ListEntry>
            <ListItems>
              <ListItem>
                <PropertyName>Name</PropertyName>
              </ListItem>
              <ListItem>
                <PropertyName>NetworkId</PropertyName>
              </ListItem>
              <ListItem>
                <PropertyName>NetworkType</PropertyName>
              </ListItem>
              <ListItem>
                <PropertyName>Overlapping</PropertyName>
              </ListItem>
              <ListItem>
                <PropertyName>NetworkSite</PropertyName>
              </ListItem>
              <ListItem>
                <PropertyName>VmmLogicalNetwork</PropertyName>
              </ListItem>
              <ListItem>
                <PropertyName>ProviderAddressSpace</PropertyName>
              </ListItem>
              <ListItem>
                <PropertyName>CustomerAddressSpace</PropertyName>
              </ListItem>
              <ListItem>
                <PropertyName>VlanId</PropertyName>
              </ListItem>
              <ListItem>
                <PropertyName>Owner</PropertyName>
              </ListItem>
            </ListItems>
          </ListEntry>
        </ListEntries>
      </ListControl>
    </View>

    <View>
      <Name>IpamSubnetTable</Name>
      <ViewSelectedBy>
        <SelectionSetName>IpamSubnet</SelectionSetName>
      </ViewSelectedBy>
      <TableControl>
        <TableHeaders>
          <TableColumnHeader>
            <label>Name</label>
          </TableColumnHeader>
          <TableColumnHeader>
            <label>NetworkId</label>
          </TableColumnHeader>
          <TableColumnHeader>
