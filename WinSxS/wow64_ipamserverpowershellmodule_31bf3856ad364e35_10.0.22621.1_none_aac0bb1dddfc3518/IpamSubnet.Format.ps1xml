      <Name>IpamFreeSubnetListView</Name>
      <ViewSelectedBy>
        <SelectionSetName>IpamFreeSubnet</SelectionSetName>
      </ViewSelectedBy>
      <ListControl>
        <ListEntries>
          <ListEntry>
            <ListItems>
              <ListItem>
                <PropertyName>NetworkId</PropertyName>
              </ListItem>
            </ListItems>
          </ListEntry>
        </ListEntries>
      </ListControl>
    </View>


    <View>
      <Name>IpamFreeSubnetTable</Name>
      <ViewSelectedBy>
        <SelectionSetName>IpamFreeSubnet</SelectionSetName>
      </ViewSelectedBy>

      <TableControl>
        <TableHeaders>
          <TableColumnHeader>
