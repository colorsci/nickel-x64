              </ListItem>
              <ListItem>
                <PropertyName>DnsRecordStatus</PropertyName>
              </ListItem>
            </ListItems>
          </ListEntry>
        </ListEntries>
      </ListControl>
    </View>


    <View>
      <Name>IpamFreeAddressTable</Name>
      <ViewSelectedBy>
        <SelectionSetName>IpamFreeAddress</SelectionSetName>
      </ViewSelectedBy>

      <TableControl>
