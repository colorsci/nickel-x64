<Configuration>
  <SelectionSets>
    <SelectionSet>
      <Name>IpamAddress</Name>
      <Types>
        <TypeName>Microsoft.Management.Infrastructure.CimInstance#MSFT_IPAM_Address</TypeName>
      </Types>
    </SelectionSet>
    <SelectionSet>
      <Name>IpamFreeAddress</Name>
      <Types>
        <TypeName>Microsoft.Management.Infrastructure.CimInstance#MSFT_IPAM_FreeAddress</TypeName>
      </Types>
    </SelectionSet>
  </SelectionSets>

  <ViewDefinitions>
    <!-- This section (first view matching MSFT_IPAM_Address type name) defines default way for displaying MSFT_IPAM_Address instances    
    -->
    <View>
      <Name>IpamAddressListView</Name>
      <ViewSelectedBy>
        <SelectionSetName>IpamAddress</SelectionSetName>
      </ViewSelectedBy>
      <ListControl>
        <ListEntries>
          <ListEntry>
            <ListItems>
              <ListItem>
                <PropertyName>IpAddress</PropertyName>
              </ListItem>
              <ListItem>
                <PropertyName>NetworkType</PropertyName>
              </ListItem>
              <ListItem>
                <PropertyName>Duplicate</PropertyName>
              </ListItem>
              <ListItem>
                <PropertyName>ExpiryStatus</PropertyName>
              </ListItem>
              <ListItem>
                <PropertyName>MacAddress</PropertyName>
              </ListItem>
              <ListItem>
                <PropertyName>ManagedByService</PropertyName>
              </ListItem>
              <ListItem>
                <PropertyName>ServiceInstance</PropertyName>
              </ListItem>
              <ListItem>
                <PropertyName>DeviceName</PropertyName>
              </ListItem>
              <ListItem>
                <PropertyName>DeviceType</PropertyName>
              </ListItem>
              <ListItem>
                <PropertyName>IpAddressState</PropertyName>
              </ListItem>
              <ListItem>
                <PropertyName>AssignmentType</PropertyName>
              </ListItem>
            </ListItems>
          </ListEntry>
        </ListEntries>
      </ListControl>
    </View>


    <View>
      <Name>IpamAddressTable</Name>
      <ViewSelectedBy>
        <SelectionSetName>IpamAddress</SelectionSetName>
      </ViewSelectedBy>

      <TableControl>
        <TableHeaders>
