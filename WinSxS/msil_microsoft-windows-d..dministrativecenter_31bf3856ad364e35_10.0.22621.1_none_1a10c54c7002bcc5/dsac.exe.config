<?xml version="1.0"?>
<configuration>
  <configSections>
    <!-- Enabling support for International Resource Identifiers (IRIs).  -->
    <section name="uri" type="System.Configuration.UriSection, System, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089"/>
  </configSections>
  <!-- Applying IRI parsing rules. -->
  <uri>
    <iriParsing enabled="true"/>
  </uri>
  <startup>
    <requiredRuntime version="4.0.0.0"/>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5"/>
  </startup>
  
  <!--
  The following controls system trace logging. The log level can be
  None|Error|Warning|Info|Verbose.
  
  <appSettings>
    <add key="DsacLogLevel" value="Info" />
  </appSettings>
  -->

</configuration>
