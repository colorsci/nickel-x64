    <!-- sample UA "Mozilla/2.0 (compatible; Elaine/1.0)" -->
    <browser id="Mypalm" parentID="Mozilla">
        <identification>
            <userAgent match="Mozilla/2\.0 \(compatible; Elaine/(?'gatewayMajorVersion'\w*)(?'gatewayMinorVersion'\.\w*)\)" />
        </identification>

        <capture>
        </capture>

        <capabilities>
