  <asmv2:configuration xmlns:asmv2="urn:schemas-microsoft-com:asm.v3" xmlns:wcm="http://schemas.microsoft.com/WMIConfig/2002/State">
    <asmv2:configurationSchema>
      <xsd:schema xmlns="Microsoft-Windows-DirectX-DirectPlay8" targetNamespace="Microsoft-Windows-DirectX-DirectPlay8">
        <xsd:element name="BaseDPlayPort" type="xsd:unsignedInt" wcm:description="DEBUG: Starting port number to use when binding sockets to the DirectPlay port range." wcm:displayName="Base DirectPlay8 Port" wcm:handler="regkey('HKEY_LOCAL_MACHINE\SOFTWARE\Microsoft\DirectPlay8')" />
        <xsd:element name="CacheLifeFound" type="xsd:unsignedInt" wcm:description="DEBUG: How long, in milliseconds, to cache IDirectPlayNATHelp::QueryAddress results where the address was found." wcm:displayName="Cache Life Found" wcm:handler="regkey('HKEY_LOCAL_MACHINE\SOFTWARE\Microsoft\DirectPlayNATHelp\DPNHUPnP')" />
        <xsd:element name="CacheLifeNotFound" type="xsd:unsignedInt" wcm:description="DEBUG: How long, in milliseconds, to cache IDirectPlayNATHelp::QueryAddress results where the address was not found." wcm:displayName="Cache Life Not Found" wcm:handler="regkey('HKEY_LOCAL_MACHINE\SOFTWARE\Microsoft\DirectPlayNATHelp\DPNHUPnP')" />
        <xsd:element name="DefaultIPAddressFamilySetting" type="xsd:unsignedInt" wcm:description="DEBUG: Address family to use for all applications. This is used to restrict the IPv4/IPv6 hybrid service provider to only IPv4 or IPv6. 0: Use both IPv4 and IPv6. 2: Use only IPv4. 23: Use only IPv6." wcm:displayName="Default IP Address Family Setting" wcm:handler="regkey('HKEY_LOCAL_MACHINE\SOFTWARE\Microsoft\DirectPlay8')" wcm:legacyName="IPAddressFamilySettings" />
        <xsd:element name="DefaultNATResolver" type="xsd:string" wcm:description="DEBUG: Comma separated list of default NAT resolver servers to use. The servers must be in the form of “hostname:port”, and separated by a comma. No user string will be sent to the servers listed here." wcm:displayName="Default NAT Resolver Servers" wcm:handler="regkey('HKEY_LOCAL_MACHINE\SOFTWARE\Microsoft\DirectPlay8')" />
        <xsd:element name="DefaultPathTestCount" type="xsd:unsignedInt" wcm:description="DEBUG: Default number of path test messages to send." wcm:displayName="Default Path Test Count" wcm:handler="regkey('HKEY_LOCAL_MACHINE\SOFTWARE\Microsoft\DirectPlay8')" />
        <xsd:element name="DefaultPathTestInterval" type="xsd:unsignedInt" wcm:description="DEBUG: Default interval between each path test message, in milliseconds." wcm:displayName="Default Path Test Interval" wcm:handler="regkey('HKEY_LOCAL_MACHINE\SOFTWARE\Microsoft\DirectPlay8')" />
        <xsd:element name="DefaultProtocolCoalesceMode" type="xsd:unsignedInt" wcm:description="DEBUG: Coalescence settings to use for all applications. This is used to override an application’s coalescence settings for every send. 0: Use application's programmatic settings. 1: Explicitly enable coalescence. 2: Explicitly disable coalescence." wcm:displayName="Default Protocol Coalesce Mode" wcm:handler="regkey('HKEY_LOCAL_MACHINE\SOFTWARE\Microsoft\DirectPlay8')" wcm:legacyName="ProtocolCoalesceMode" />
        <xsd:element name="DefaultProtocolLinkTuningMode" type="xsd:unsignedInt" wcm:description="DEBUG: Link tuning settings to use for all applications. This is used to override an application’s link tuning settings." wcm:displayName="Default Protocol Link Tuning Mode" wcm:handler="regkey('HKEY_LOCAL_MACHINE\SOFTWARE\Microsoft\DirectPlay8')" wcm:legacyName="ProtocolLinkTuningMode" />
        <xsd:element name="DefaultProtocolSigningMode" type="xsd:unsignedInt" wcm:description="DEBUG:Signing mode to use for all applications. This is used to override an application’s signing mode. 0: Use application's programmatic settings. 1: Explicitly enable fast signing. 2: Explicitly enable full signing. 3: Explicitly disable signing." wcm:displayName="Default Protocol Signing Mode" wcm:handler="regkey('HKEY_LOCAL_MACHINE\SOFTWARE\Microsoft\DirectPlay8')" wcm:legacyName="ProtocolSigningMode" />
        <xsd:element name="DefaultTraversalModeSetting" type="xsd:unsignedInt" wcm:description="DEBUG: Traversal mode to use for all applications. This is used to determine the traversal mode for mapping ports on a NAT/firewall. 0: Do not map ports on NAT/Firewall. 1: Same port number must be mapped on NAT/Firewall as is used locally. 2: Try to map same port on NAT/Firewall as is being used locally. 0x80000000: Mask with previous values to override application specification of traversal mode in address." wcm:displayName="Default Traversal Mode Setting" wcm:handler="regkey('HKEY_LOCAL_MACHINE\SOFTWARE\Microsoft\DirectPlay8')" wcm:legacyName="TraversalModeSettings" />
        <xsd:element name="DefaultWinsockSendBufferSize" type="xsd:unsignedInt" wcm:description="DEBUG: Send buffer size to use for all applications. This is used to override the standard Winsock send buffer size. The value is in bytes, where 0 means no buffer and 0xFFFFFFFF means use the default Winsock settings." wcm:displayName="Default WinsockSendBufferSize" wcm:handler="regkey('HKEY_LOCAL_MACHINE\SOFTWARE\Microsoft\DirectPlay8')" wcm:legacyName="WinsockSendBufferSize" />
        <xsd:element name="DisableDPNHFirewallSupport" type="xsd:boolean" wcm:description="DEBUG: Boolean indicating whether local firewall support for all DirectPlayNATHelp providers should be disabled." wcm:displayName="Disable DPNH Firewall Support" wcm:handler="regkey('HKEY_LOCAL_MACHINE\SOFTWARE\Microsoft\DirectPlay8')" />
        <xsd:element name="DisableDPNHGatewaySupport" type="xsd:boolean" wcm:description="DEBUG: Boolean indicating whether gateway support for all DirectPlayNATHelp providers should be disabled." wcm:displayName="Disable DPNH Gateway Support" wcm:handler="regkey('HKEY_LOCAL_MACHINE\SOFTWARE\Microsoft\DirectPlay8')" />
        <xsd:element name="DontSendAddressesWithPathTests" type="xsd:boolean" wcm:description="DEBUG: Boolean enabling or disabling the automatic inclusion of the original source and destination socket addresses for path test messages sent by debug versions of dpnet.dll." wcm:displayName="Don't Send Addresses With Path Tests" wcm:handler="regkey('HKEY_LOCAL_MACHINE\SOFTWARE\Microsoft\DirectPlay8')" />
        <xsd:element name="ExtendedFrameSize" type="xsd:unsignedInt" wcm:description="DEBUG: Allows Winsock 2 capable service providers to send and receive oversized frames. This value increases the maximum allowed frame by the specified number of bytes." wcm:displayName="Extended Frame Size" wcm:handler="regkey('HKEY_LOCAL_MACHINE\SOFTWARE\Microsoft\DirectPlay8')" />
        <xsd:element name="GatewayV4" type="xsd:unsignedInt" wcm:description="DEBUG: Override for using a different gateway IP address for locating a remote UPnP device when IPHLPAPI cannot be loaded." wcm:displayName="Gateway V4" wcm:handler="regkey('HKEY_LOCAL_MACHINE\SOFTWARE\Microsoft\DirectPlayNATHelp\DPNHUPnP')" />
        <xsd:element name="HNetFWAPIMode" type="xsd:unsignedInt" wcm:description="DEBUG: Used to control force HomeNet firewall API traversal behavior for all components that load DPNHUPNP, regardless of the hard coded values they pass to IDirectPlayNATHelp::Initialize. 0: Use component's settings. 1: Force firewall traversal on. 2: Force firewall traversal off." wcm:displayName="HNet FWAPI Mode" wcm:handler="regkey('HKEY_LOCAL_MACHINE\SOFTWARE\Microsoft\DirectPlayNATHelp\DPNHUPnP')" />
        <xsd:element name="KeepPollingForRemoteGateway" type="xsd:boolean" wcm:description="DEBUG: Boolean indicating whether remote UPnP device discovery messages should continue to be sent regularly after the first IDirectPlayNATHelp::GetCaps call." wcm:displayName="Keep Polling for Remote Gateway" wcm:handler="regkey('HKEY_LOCAL_MACHINE\SOFTWARE\Microsoft\DirectPlayNATHelp\DPNHUPnP')" />
        <xsd:element name="MapUPnPDiscoverySocket" type="xsd:boolean" wcm:description="DEBUG: Boolean indicating whether the UPnP device discovery socket should be mapped on the local firewall.  This controls support for the scenario where this machine is behind a NAT and also has a local firewall enabled that doesn’t make automatic outbound mappings." wcm:displayName="Map UPnP DiscoverySocket" wcm:handler="regkey('HKEY_LOCAL_MACHINE\SOFTWARE\Microsoft\DirectPlayNATHelp\DPNHUPnP')" />
        <xsd:element name="MaxDPlayPort" type="xsd:unsignedInt" wcm:description="DEBUG: Maximum port number to use when binding sockets to the DirectPlay port range." wcm:displayName="Max DirectPlay8 Port" wcm:handler="regkey('HKEY_LOCAL_MACHINE\SOFTWARE\Microsoft\DirectPlay8')" />
        <xsd:element name="MaxEnumDataSize" type="xsd:unsignedInt" wcm:description="DEBUG: Overrides the default maximum enumeration query/response data frame size used, in bytes." wcm:displayName="Max Enum Data Size" wcm:handler="regkey('HKEY_LOCAL_MACHINE\SOFTWARE\Microsoft\DirectPlay8')" />
        <xsd:element name="MaxPollInterval" type="xsd:unsignedInt" wcm:description="DEBUG: Maximum number of milliseconds to which the IDirectPlayNATHelp::GetCaps recommended interval can back off.  This must be larger than the base interval (see the NoActiveNotifyPollInterval value)." wcm:displayName="Max Poll Interval" wcm:handler="regkey('HKEY_LOCAL_MACHINE\SOFTWARE\Microsoft\DirectPlayNATHelp\DPNHUPnP')" />
        <xsd:element name="MaxUserDataSize" type="xsd:unsignedInt" wcm:description="DEBUG: Overrides the default maximum user data frame size used, in bytes." wcm:displayName="Max User Data Size" wcm:handler="regkey('HKEY_LOCAL_MACHINE\SOFTWARE\Microsoft\DirectPlay8')" />
        <xsd:element name="MinUpdateServerStatusInterval" type="xsd:unsignedInt" wcm:description="DEBUG: Specifies the minimum amount of time in milliseconds that must elapse between IDirectPlayNATHelp::GetCaps calls before performing the lengthy network operations (other than automatic lease extensions) are allowed." wcm:displayName="Min Update Server Status Interval" wcm:handler="regkey('HKEY_LOCAL_MACHINE\SOFTWARE\Microsoft\DirectPlayNATHelp\DPNHUPnP')" />
        <xsd:element name="MulticastTTL" type="xsd:unsignedInt" wcm:description="DEBUG: Changes the multicast TTL setting for UPnP device discovery sockets in use by DPNHUPNP.  If the value is 0, the default TCP/IP settings (normally 1) will be used." wcm:displayName="Multicast TTL" wcm:handler="regkey('HKEY_LOCAL_MACHINE\SOFTWARE\Microsoft\DirectPlayNATHelp\DPNHUPnP')" />
        <xsd:element name="NATResolverQueryCount" type="xsd:unsignedInt" wcm:description="DEBUG: Number of NAT resolver messages to send." wcm:displayName="NAT Resolver Query Count" wcm:handler="regkey('HKEY_LOCAL_MACHINE\SOFTWARE\Microsoft\DirectPlay8')" />
        <xsd:element name="NATResolverQueryTimeout" type="xsd:unsignedInt" wcm:description="DEBUG: Default duration to wait after sending the last NAT resolver query message, in milliseconds." wcm:displayName="NAT Resolver Query Timeout" wcm:handler="regkey('HKEY_LOCAL_MACHINE\SOFTWARE\Microsoft\DirectPlay8')" />
        <xsd:element name="NoActiveNotifyPollInterval" type="xsd:unsignedInt" wcm:description="DEBUG: Selects the initial interval in milliseconds at which DPNHUPNP recommends its caller update the server status (see IDirectPlayNATHelp::GetCaps) unless a lease expires earlier.  This value is backed off a random amount each time (see the PollIntervalBackoff value)." wcm:displayName="No Active Notify Poll Interval" wcm:handler="regkey('HKEY_LOCAL_MACHINE\SOFTWARE\Microsoft\DirectPlayNATHelp\DPNHUPnP')" />
        <xsd:element name="NoAsymmetricMappings" type="xsd:boolean" wcm:description="DEBUG: Boolean indicating whether asymmetric UPnP port mappings can be attempted." wcm:displayName="No Asymmetric Mappings" wcm:handler="regkey('HKEY_LOCAL_MACHINE\SOFTWARE\Microsoft\DirectPlayNATHelp\DPNHUPnP')" />
        <xsd:element name="NoBroadcastMappings" type="xsd:boolean" wcm:description="DEBUG: Boolean overriding whether the broadcast address is to be used when mapping shared ports." wcm:displayName="No Broadcast Mappings" wcm:handler="regkey('HKEY_LOCAL_MACHINE\SOFTWARE\Microsoft\DirectPlayNATHelp\DPNHUPnP')" />
        <xsd:element name="PASTCacheLifeFound" type="xsd:unsignedInt" wcm:description="DEBUG: How long, in milliseconds, to cache IDirectPlayNATHelp::QueryAddress results where the address was found." wcm:displayName="DPNHPAST Cache Life Found" wcm:handler="regkey('HKEY_LOCAL_MACHINE\SOFTWARE\Microsoft\DirectPlayNATHelp\DPNHPAST')" wcm:legacyName="CacheLifeFound" />
        <xsd:element name="PASTCacheLifeNotFound" type="xsd:unsignedInt" wcm:description="DEBUG: How long, in milliseconds, to cache IDirectPlayNATHelp::QueryAddress results where the address was not found." wcm:displayName="DPNHPAST Cache Life Not Found" wcm:handler="regkey('HKEY_LOCAL_MACHINE\SOFTWARE\Microsoft\DirectPlayNATHelp\DPNHPAST')" wcm:legacyName="CacheLifeNotFound" />
        <xsd:element name="PASTGatewayV4" type="xsd:unsignedInt" wcm:description="DEBUG: Override for using a different gateway IP address for locating a remote PAST server when IPHLPAPI cannot be loaded." wcm:displayName="DPNHPAST Gateway V4" wcm:handler="regkey('HKEY_LOCAL_MACHINE\SOFTWARE\Microsoft\DirectPlayNATHelp\DPNHPAST')" wcm:legacyName="GatewayV4" />
        <xsd:element name="PASTICSMode" type="xsd:unsignedInt" wcm:description="DEBUG: Used to control NAT/Gateway traversal behavior for all components that load DPNHPAST, regardless of the hard coded values they pass to IDirectPlayNATHelp::Initialize. 0: Use component's settings. 1: Force NAT/Gateway traversal on. 2: Force NAT/Gateway traversal off." wcm:displayName="PASTICS Mode" wcm:handler="regkey('HKEY_LOCAL_MACHINE\SOFTWARE\Microsoft\DirectPlayNATHelp\DPNHPAST')" />
        <xsd:element name="PASTKeepPollingForRemoteGateway" type="xsd:boolean" wcm:description="DEBUG: Boolean indicating whether remote PAST server registration messages should continue to be sent regularly after the first IDirectPlayNATHelp::GetCaps call." wcm:displayName="DPNHPAST Keep Polling For Remote Gateway" wcm:handler="regkey('HKEY_LOCAL_MACHINE\SOFTWARE\Microsoft\DirectPlayNATHelp\DPNHPAST')" wcm:legacyName="KeepPollingForRemoteGateway" />
        <xsd:element name="PASTMaxPollInterval" type="xsd:unsignedInt" wcm:description="DEBUG: Maximum number of milliseconds to which the IDirectPlayNATHelp::GetCaps recommended interval can back off.  This must be larger than the base interval (see the NoActiveNotifyPollInterval value)." wcm:displayName="DPNHPAST Max Poll Interval" wcm:handler="regkey('HKEY_LOCAL_MACHINE\SOFTWARE\Microsoft\DirectPlayNATHelp\DPNHPAST')" wcm:legacyName="MaxPollInterval" />
        <xsd:element name="PASTMinUpdateServerStatusInterval" type="xsd:unsignedInt" wcm:description="DEBUG: Specifies the minimum amount of time in milliseconds that must elapse between IDirectPlayNATHelp::GetCaps calls before performing the lengthy network operations (other than automatic lease extensions) are allowed." wcm:displayName="DPNHPAST Min Update Server Status Interval" wcm:handler="regkey('HKEY_LOCAL_MACHINE\SOFTWARE\Microsoft\DirectPlayNATHelp\DPNHPAST')" wcm:legacyName="MinUpdateServerStatusInterval" />
        <xsd:element name="PASTNoActiveNotifyPollInterval" type="xsd:unsignedInt" wcm:description="DEBUG: Selects the interval in milliseconds at which DPNHPAST recommends its caller update the server status (See IDirectPlayNATHelp::GetCaps) unless a lease expires earlier." wcm:displayName="DPNHPAST No Active Notify Poll Interval" wcm:handler="regkey('HKEY_LOCAL_MACHINE\SOFTWARE\Microsoft\DirectPlayNATHelp\DPNHPAST')" wcm:legacyName="NoActiveNotifyPollInterval" />
        <xsd:element name="PASTPFWMode" type="xsd:unsignedInt" wcm:description="DEBUG: Used to control force HomeNet firewall API traversal behavior for all components that load DPNHPAST, regardless of the hard coded values they pass to IDirectPlayNATHelp::Initialize. 0: Use component's settings. 1: Force firewall traversal on. 2: Force firewall traversal off." wcm:displayName="PASTPFW Mode" wcm:handler="regkey('HKEY_LOCAL_MACHINE\SOFTWARE\Microsoft\DirectPlayNATHelp\DPNHPAST')" />
        <xsd:element name="PASTPollIntervalBackoff" type="xsd:unsignedInt" wcm:description="DEBUG: Upper bound for the random amount used to increase the IDirectPlayNATHelp::GetCaps recommended interval when the network topology stays the same.  The interval starts at a base value (see the NoActiveNotifyPollInterval value) and each call to GetCaps adds a random number of milliseconds between 0 and the value to the recommendation." wcm:displayName="DPNHPAST Poll Interval Backoff" wcm:handler="regkey('HKEY_LOCAL_MACHINE\SOFTWARE\Microsoft\DirectPlayNATHelp\DPNHPAST')" wcm:legacyName="PollIntervalBackoff" />
