  </VPNv2>

  <EnterpriseAPN>
    <category name="Connectivity" />
  </EnterpriseAPN>

  <eUICCs>
    <category name="Connectivity" />
  </eUICCs>

  <MultiSIM>
    <category name="Connectivity" />
  </MultiSIM>

  <CleanPC>
    <category name="CleanPC" />
  </CleanPC>

  <SUPL>
    <category name="InitialCustomization" />
  </SUPL>

  <PassportForWork>
    <category name="Security" />
  </PassportForWork>

  <DeviceUpdateCenter>
    <category name="Policies" />
  </DeviceUpdateCenter>

  <LanguagePackManagement>
    <category name="Languages" />
  </LanguagePackManagement>

</categoryxml>
