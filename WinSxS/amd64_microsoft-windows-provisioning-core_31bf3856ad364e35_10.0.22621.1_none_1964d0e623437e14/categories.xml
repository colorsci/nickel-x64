  </Personalization>

  <AssignedAccess>
    <category name="UxLockdown" />
  </AssignedAccess>

  <DeveloperSetup>
    <category name="InitialCustomization" />
  </DeveloperSetup>

  <DMAcc>
    <category name="DeviceManagement" />
  </DMAcc>

  <DMClient>
    <UpdateManagementServiceAddress>
      <category name="DeviceManagement" />
    </UpdateManagementServiceAddress>
  </DMClient>

  <People>
    <Phonebook>
      <category name="Content" />
    </Phonebook>
  </People>

  <NetworkProxy>
    <category name="Connectivity"/>
  </NetworkProxy>

  <NetworkQoSPolicy>
    <category name="Connectivity"/>
  </NetworkQoSPolicy>

  <SurfaceHub>
